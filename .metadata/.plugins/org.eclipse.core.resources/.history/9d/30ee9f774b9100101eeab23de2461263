package Association;

class Person {
	String name ;
	int salary;
	Person (String name, int salary ){
		this.name =  name ;
		this.salary= salary;
	}
}
class Job{
	Person p;
    Job(Person p){
    	this.p = p;
    }
    void show() {
       System.out.println(p.name+"\n"+p.salary);
    }
   }
public class Association {
      public static void main(String[] args) {
		Person p  = new Person("Hashim Ansari;",10000);
		Job j  = new Job(p);
		j.show();
	}  
}
