package com.abstraction;

abstract class Cat
{
   abstract void run () ; 
   void eat()
{
   System.out.println("Cat eat Rat");	   
}
   }
interface Cow
{
     void gave();
    
}
class Animal extends Cat implements Cow{
	
	public void run () {
		super.eat();
		System.out.println("Cat Is Running");
	}
	public void gave (){
		System.out.println("Cow Gave Milk");
	}
}

public class AbstractInterface {

	public static void main(String[] args) {
	     Animal  a = new Animal ();
	     a.run();
	     a.gave();
	}

}
