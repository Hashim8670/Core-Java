package String;

public class String_Method {

	public static void main(String[] args) {
		             // 1st method
		String str="       Hashim			";
		System.out.println(str.length()); 
	
		int i=str.length();
		if(str.length()==0) {
			System.out.println("Please Enter password");
		}
		else { 
			System.out.println("You Entered Password Successfully");
		
		}
		      //    2nd method
		String s="";
		if(s.isEmpty()){
			System.out.println("Please Enter password ");
		}
		else { 
			System.out.println("You Entered Password Successfully ");
		
		}
		   //  3rs method
		 String _1st="a";
		 String _2nd="A";
		 System.out.println(_1st.equals(_2nd));
		 
			
	}

}
