package String;

public class String_Method {

	public static void main(String[] args) {
		
		String str="Hashim";
		String s="Ansari";
		System.out.println(str.length());
	
		int i=str.length();
		if(i==0) {
			System.out.println("Please Enter password");
		}
		else { 
			System.out.println("You Entered Password Successfully");
		
		}
		System.out.println(s.isEmpty());
		boolean b=true;
		if(s.isEmpty()==b){
			System.out.println("Please Enter password 1");
		}
		else { 
			System.out.println("You Entered Password Successfully 1");
		
		}
			
	}

}
