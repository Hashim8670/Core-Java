package Inharitance;

interface Weapon{
	void use();
}
interface Character{
	void attack();
}
abstract class Warrior implements  Weapon,Character{
	public void use() {
		System.out.println("Warrior use a Sword");
	}
	public void attack() {
		System.out.println("Attack the Enemy");
	}
}
class Main extends Warrior{
	     void kill() {
	    	 System.out.println("And kill him");
	     }
} 
public class Multiple_Inharitance {

	public static void main(String[] args) {
		Main main = new Main();
        main.use();
        main.attack();
        main.kill();
	}

}
