package com.core.java;
class Building {
    String name; 
     String detail;
     Building (String n,String d){
        name=n;
        detail= d;
     }
     void display(){
         System.out .println(name +detail);
}
class Room{
	
     Building b;
    String roomname ;
    String roomdetail;
    Room(String rn, String rd){
        roomname = rn;
        roomdetail=  rd;
        Building b = new Building("Hashim Department","100 BHK");
    }
    void show(){
    	b.display();
        System.out.println(roomname + roomdetail);
    }
    public class Composition2{
        public static void mmain (String args[]){
            Room r =  new Room("Hall","1 BHK");
            r.show();
        } 
    }
}
