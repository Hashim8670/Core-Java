
package String;

public class StringMethod {

	public static void main(String[] args) {
	
      String str="hashim"; 
      String str1="hashim"; 
     
      String s=new String("hashim");
      String s1=new String("hashim");

     
     System.out.println("1--> "+(str==str1)); 
     System.out.println("2--> "+(str==s)); 
     System.out.println("3--> "+s.equals(s1)); 
     System.out.println("4--> "+str.equals(s1)); 
     System.out.println("5--> "+str.length()); 
     System.out.println("6--> "+str.toUpperCase()); 
     System.out.println("7--> "+str.toLowerCase()); 
     System.out.println("8--> "+str.contains("h")); 
     System.out.println("9--> "+str.indexOf("h"));  
     System.out.println("10--> "+str.lastIndexOf(""));
//     System.out.println("11--> "+contains()); 
//     System.out.println("12--> "+contains()); 
//     System.out.println("13--> "+contains()); 
//     System.out.println("14--> "+contains()); 
//     System.out.println("15--> "+contains()); 
//     System.out.println("16--> "+contains());
//     System.out.println("17--> "+contains()); 
     
	}

}
